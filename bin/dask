#!/usr/bin/env python3.13
"""
dask - Send message to Droid.

NOTE: Droid Terminal backend is not yet implemented in Gateway.
This command is currently disabled.
"""

from __future__ import annotations

import sys
from pathlib import Path

script_dir = Path(__file__).resolve().parent
lib_dir = script_dir.parent / "lib"
sys.path.insert(0, str(lib_dir))

from cli_output import EXIT_ERROR


def main(argv: list[str]) -> int:
    print("[ERROR] dask (Droid) is not yet supported in Gateway mode.", file=sys.stderr)
    print("Terminal backend implementation is pending.", file=sys.stderr)
    return EXIT_ERROR


if __name__ == "__main__":
    sys.exit(main(sys.argv))
